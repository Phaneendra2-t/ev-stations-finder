# OpenChargeMap API Key (Optional - works without key but with rate limits)
# Get your free key at: https://openchargemap.org/site/develop/api
VITE_OPENCHARGEMAP_API_KEY=your_api_key_here

# Google Maps API Key (Optional - for enhanced routing features)
# Get your key at: https://console.cloud.google.com/
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# App Configuration
VITE_APP_NAME=EV Charging Station Finder
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_MOCK_DATA=false

# API Configuration
VITE_API_TIMEOUT=10000
VITE_MAX_STATIONS=50
VITE_DEFAULT_RADIUS=25
